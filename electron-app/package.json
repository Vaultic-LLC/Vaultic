{
	"name": "Vaultic",
	"version": "1.0.3",
	"description": "A Password Manager",
	"main": "./out/main/index.js",
	"author": "Vaultic LLC",
	"homepage": "https://vaultic.org",
	"repository": {
		"type": "git",
		"url": "https://github.com/Vaultic-LLC/Vaultic"
	},
	"scripts": {
		"copy-core-main": "node ../copy.js src=../core-main/src dest=./src/main/Core",
		"copy-core-renderer": "node ../copy.js src=../core-renderer/src dest=./src/renderer/src/core",
		"copy": "npm run copy-core-main && npm run copy-core-renderer",
		"format": "prettier --write .",
		"lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
		"typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
		"typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
		"typecheck": "npm run typecheck:node && npm run typecheck:web",
		"start": "electron-vite preview",
		"dev": "npm run copy && electron-vite dev",
		"build": "npm run copy && electron-vite build",
		"postinstall": "electron-builder install-app-deps",
		"build:win": "npm run build && electron-builder --win --config",
		"build:mac": "npm run build && electron-builder --mac --config",
		"build:linux": "npm run build && electron-builder --linux --config",
		"package": "npm run copy && electron-vite build --outDir=dist && electron-forge package",
		"make": "npm run copy && electron-vite build --outDir=dist && electron-forge make",
		"copy-core-main-test": "node ../../copy.js src=../../core-main/src dest=./src/main/Core",
		"copy-core-renderer-test": "node ../../copy.js src=../../core-renderer/src dest=./src/renderer/src/core",
		"copy-test": "npm run copy-core-main-test && npm run copy-core-renderer-test",
		"test": "npm run copy-test && electron-vite dev",
		"rebuild": "electron-rebuild -f",
		"typeorm": "typeorm-ts-node-commonjs"
	},
	"dependencies": {
		"@electron-toolkit/preload": "2.0.0",
		"@electron-toolkit/utils": "2.0.0",
		"@electron/asar": "3.2.8",
		"@esbuild-plugins/node-globals-polyfill": "0.2.3",
		"@esbuild-plugins/node-modules-polyfill": "0.2.2",
		"@melloware/coloris": "0.24.0",
		"@noble/curves": "1.4.2",
		"@noble/post-quantum": "0.4.0",
		"@popperjs/core": "2.11.8",
		"@primeuix/themes": "^1.2.5",
		"@serenity-kit/opaque": "0.9.0",
		"@tweenjs/tween.js": "21.0.0",
		"axios": "1.12.1",
		"better-sqlite3": "^12.4.1",
		"chart.js": "4.4.0",
		"chartjs-plugin-zoom": "2.0.1",
		"csv-parse": "5.5.6",
		"csv-stringify": "6.5.0",
		"electron-squirrel-startup": "^1.0.1",
		"electron-updater": "6.6.2",
		"getmac": "6.6.0",
		"jose": "5.6.3",
		"primeicons": "7.0.0",
		"primevue": "4.4.0",
		"qrcode": "1.5.3",
		"quill": "2.0.2",
		"reflect-metadata": "0.2.2",
		"sodium-native": "4.3.3",
		"sqlite": "5.1.1",
		"sqlite3": "^5.1.7",
		"tar-fs": "^2.1.4",
		"tippy.js": "6.3.7",
		"typeorm": "^0.3.21",
		"update-electron-app": "^3.1.1",
		"v-calendar-tw": "3.2.0",
		"vite-plugin-babel": "^1.3.0",
		"vue-chartjs": "5.2.0"
	},
	"devDependencies": {
		"@babel/plugin-transform-class-properties": "^7.25.9",
		"@babel/plugin-transform-classes": "^7.25.9",
		"@babel/plugin-transform-object-rest-spread": "^7.25.9",
		"@electron-forge/cli": "^7.9.0",
		"@electron-forge/maker-dmg": "^7.9.0",
		"@electron-forge/maker-squirrel": "^7.9.0",
		"@electron-toolkit/eslint-config": "1.0.1",
		"@electron-toolkit/eslint-config-ts": "1.0.0",
		"@electron-toolkit/tsconfig": "1.0.1",
		"@electron/rebuild": "3.6.0",
		"@rushstack/eslint-patch": "1.3.3",
		"@types/node": "18.17.5",
		"@types/pg": "^8.10.9",
		"@vaultic/shared": "../core-shared",
		"@vitejs/plugin-vue": "5.2.3",
		"@vue/eslint-config-prettier": "8.0.0",
		"@vue/eslint-config-typescript": "11.0.3",
		"dotenv": "^17.2.3",
		"electron": "36.9.3",
		"electron-builder": "24.13.3",
		"electron-vite": "^3.1.0",
		"eslint": "8.47.0",
		"eslint-plugin-vue": "9.17.0",
		"less": "4.2.0",
		"pg": "^8.11.3",
		"prettier": "3.0.2",
		"ts-node": "10.9.2",
		"typescript": "5.1.6",
		"vite": "6.3.6",
		"vue": "3.5",
		"vue-tsc": "2.2.8"
	}
}
