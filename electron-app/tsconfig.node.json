{
	"extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
	"include": [
		"electron.vite.config.*",
		"src/main/*",
		"src/main/Objects/Files/*",
		"src/main/Objects/Server/*",
		"src/main/Types/*",
		"src/main/Objects/*",
		"src/main/Helpers/*",
		"src/main/Utilities/*",
		"src/preload/*",
		"src/preload/Types/*",
	],
	"compilerOptions": {
		"composite": true,
		"types": [
			"electron-vite/node"
		],
		"noUnusedLocals": false,
		"noUnusedParameters": false,
	}
}